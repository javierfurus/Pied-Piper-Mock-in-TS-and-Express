{
  "name": "siliconvalley-express",
  "author": "Javier Furus",
  "license": "ISC",
  "version": "1.0.0",
  "description": "Silicon Valley in Express",
  "scripts": {
    "build": "webpack",
    "dev": "concurrently \"webpack -w --display-error-details\" \"nodemon build/bundle.js\"",
    "start": "node build/bundle.js",
    "db:migrate": "node ./node_modules/.bin/knex migrate:up --knexfile=config/database.json",
    "db:rollback": "node ./node_modules/.bin/knex migrate:rollback --knexfile=config/database.json",
    "db:seed": "node ./node_modules/.bin/knex seed:run --knexfile=config/database.json"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.27",
    "@typescript-eslint/parser": "^3.8.0",
    "concurrently": "^4.1.2",
    "eslint-import-resolver-typescript": "^2.2.0",
    "eslint-plugin-import": "^2.22.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@types/knex": "^0.16.1",
    "@types/swagger-express-middleware": "^1.0.10",
    "autoprefixer": "^9.8.6",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.1",
    "express": "^4.17.1",
    "express-handlebars": "^5.1.0",
    "file-loader": "^6.0.0",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "handlebars-template-loader": "^1.0.0",
    "handlebars-webpack-plugin": "^1.8.1",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "knex": "^0.20.15",
    "lodash": "^4.17.19",
    "mini-css-extract-plugin": "^0.9.0",
    "mozjpeg": "^7.0.0",
    "mysql": "^2.18.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^9.0.0",
    "render-template-loader": "^1.2.3",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "source-map": "^0.7.3",
    "style-loader": "^1.2.1",
    "swagger-express-middleware": "^2.0.5",
    "ts-node": "^8.10.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^1.7.2"
  },
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/javierfurus/valleyinexpress.git"
  },
  "keywords": [
    "siliconvalley"
  ],
  "bugs": {
    "url": "https://github.com/javierfurus/valleyinexpress/issues"
  },
  "homepage": "https://github.com/javierfurus/valleyinexpress#readme"
}
